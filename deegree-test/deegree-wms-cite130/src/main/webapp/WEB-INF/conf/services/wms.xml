<?xml version="1.0" encoding="UTF-8"?>
<wms:deegreeWMS configVersion="0.5.0" xmlns:wms="http://www.deegree.org/services/wms" xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dgws="http://www.deegree.org/webservices"
  xsi:schemaLocation="http://www.deegree.org/services/wms http://schemas.deegree.org/wms/0.5.0/wms_configuration.xsd">

  <wms:ServiceConfiguration>

    <wms:UnrequestableLayer>
      <wms:Title>CITE WMS root layer</wms:Title>
      <wms:CRS>EPSG:4326 CRS:84</wms:CRS>

      <wms:RequestableLayer>
        <wms:Name>cite:BasicPolygons</wms:Name>
        <wms:Title>cite:BasicPolygons</wms:Title>
        <wms:FeatureStoreId>basicpolygons</wms:FeatureStoreId>
        <wms:DirectStyle>
          <wms:File>../styles/BasicPolygons.xml</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>cite:Forests</wms:Name>
        <wms:Title>cite:Forests</wms:Title>
        <wms:FeatureStoreId>forests</wms:FeatureStoreId>
        <wms:DirectStyle>
          <wms:File>../styles/Forests.xml</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>cite:Lakes</wms:Name>
        <wms:Title>cite:Lakes</wms:Title>
        <wms:Dimension isElevation="true">
          <wms:Property>ELEV</wms:Property>
          <wms:DefaultValue>500</wms:DefaultValue>
          <wms:NearestValue>true</wms:NearestValue>
          <wms:Extent>500,490,480</wms:Extent>
        </wms:Dimension>
        <wms:FeatureStoreId>lakes</wms:FeatureStoreId>
        <wms:DirectStyle>
          <wms:File>../styles/Lakes.xml</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>cite:Ponds</wms:Name>
        <wms:Title>cite:Ponds</wms:Title>
        <wms:FeatureStoreId>ponds</wms:FeatureStoreId>
        <wms:DirectStyle>
          <wms:File>../styles/Ponds.xml</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>cite:NamedPlaces</wms:Name>
        <wms:Title>cite:NamedPlaces</wms:Title>
        <wms:FeatureStoreId>namedplaces</wms:FeatureStoreId>
        <wms:DirectStyle>
          <wms:File>../styles/NamedPlaces.xml</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>cite:Buildings</wms:Name>
        <wms:Title>cite:Buildings</wms:Title>
        <wms:FeatureStoreId>buildings</wms:FeatureStoreId>
        <wms:DirectStyle>
          <wms:File>../styles/Buildings.xml</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>cite:BuildingCenters</wms:Name>
        <wms:Title>cite:BuildingCenters</wms:Title>
        <wms:FeatureStoreId>buildingcenters</wms:FeatureStoreId>
        <wms:DirectStyle>
          <wms:File>../styles/BuildingCenters.xml</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>cite:DividedRoutes</wms:Name>
        <wms:Title>cite:DividedRoutes</wms:Title>
        <wms:FeatureStoreId>dividedroutes</wms:FeatureStoreId>
        <wms:DirectStyle>
          <wms:File>../styles/DividedRoutes.xml</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>cite:RoadSegments</wms:Name>
        <wms:Title>cite:RoadSegments</wms:Title>
        <wms:FeatureStoreId>roadsegments</wms:FeatureStoreId>
        <wms:DirectStyle>
          <wms:File>../styles/RoadSegments.xml</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>cite:MapNeatline</wms:Name>
        <wms:Title>cite:MapNeatline</wms:Title>
        <wms:FeatureStoreId>mapneatline</wms:FeatureStoreId>
        <wms:DirectStyle>
          <wms:File>../styles/MapNeatline.xml</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>cite:Streams</wms:Name>
        <wms:Title>cite:Streams</wms:Title>
        <wms:FeatureStoreId>streams</wms:FeatureStoreId>
        <wms:DirectStyle>
          <wms:File>../styles/Streams.xml</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>cite:Bridges</wms:Name>
        <wms:Title>cite:Bridges</wms:Title>
        <wms:FeatureStoreId>bridges</wms:FeatureStoreId>
        <wms:DirectStyle>
          <wms:File>../styles/Bridges.xml</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>cite:Autos</wms:Name>
        <wms:Title>cite:Autos</wms:Title>
        <wms:Dimension isTime="true">
          <wms:Property>TIME</wms:Property>
          <wms:DefaultValue>2000-01-01T00:00:00Z</wms:DefaultValue>
          <wms:MultipleValues>true</wms:MultipleValues>
          <wms:NearestValue>true</wms:NearestValue>
          <wms:Current>true</wms:Current>
          <wms:Extent>2000-01-01T00:00:00Z/2000-01-01T00:01:00Z/PT5S</wms:Extent>
        </wms:Dimension>
        <wms:FeatureStoreId>autos</wms:FeatureStoreId>
        <wms:DirectStyle>
          <wms:File>../styles/Autos.xml</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>cite:Terrain</wms:Name>
        <wms:Title>cite:Terrain</wms:Title>
        <wms:Dimension isElevation="true">
          <wms:Property>ELEV</wms:Property>
          <wms:DefaultValue>0/425</wms:DefaultValue>
          <wms:MultipleValues>true</wms:MultipleValues>
          <wms:NearestValue>false</wms:NearestValue>
          <wms:Extent>0/425/1</wms:Extent>
        </wms:Dimension>
        <wms:CoverageStoreId>terrain</wms:CoverageStoreId>
        <wms:DirectStyle>
          <wms:File>../styles/terrain2.xml</wms:File>
        </wms:DirectStyle>
        <wms:DirectStyle>
          <wms:File>../styles/terrain1.xml</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

    </wms:UnrequestableLayer>

  </wms:ServiceConfiguration>
</wms:deegreeWMS>
