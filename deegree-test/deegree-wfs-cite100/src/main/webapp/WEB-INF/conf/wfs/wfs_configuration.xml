<?xml version="1.0" encoding="UTF-8"?>
<deegreeWFS configVersion="0.5.0" xmlns="http://www.deegree.org/services/wfs" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.deegree.org/services/wfs http://schemas.deegree.org/wfs/0.5.0/wfs_configuration.xsd">
  <ServiceConfiguration>
    <MemoryFeatureStore xmlns="http://www.deegree.org/datasource">
      <NamespaceHint prefix="cgf" namespaceURI="http://www.opengis.net/cite/geometry"></NamespaceHint>
      <NamespaceHint prefix="ccf" namespaceURI="http://www.opengis.net/cite/complex"></NamespaceHint>
      <NamespaceHint prefix="cdf" namespaceURI="http://www.opengis.net/cite/data"></NamespaceHint>
      <GMLSchemaFileURL gmlVersion="GML_2">datastores/gml/cite100/all.xsd</GMLSchemaFileURL>
      <GMLFeatureCollectionFileURL gmlVersion="GML_2">datastores/gml/cite100/dataset.xml</GMLFeatureCollectionFileURL>
    </MemoryFeatureStore>
  </ServiceConfiguration>
  <PublishedInformation>
    <SupportedVersions>
      <Version>1.0.0</Version>
    </SupportedVersions>
    <EnableTransactions>true</EnableTransactions>
    <EnableStreaming>false</EnableStreaming>
    <QuerySRS>EPSG:4326</QuerySRS>
  </PublishedInformation>
