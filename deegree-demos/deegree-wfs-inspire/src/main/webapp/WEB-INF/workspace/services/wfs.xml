<deegreeWFS xmlns="http://www.deegree.org/services/wfs" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  configVersion="0.5.0" xsi:schemaLocation="http://www.deegree.org/services/wfs http://schemas.deegree.org/wfs/0.5.0/wfs_configuration.xsd">
  <ServiceConfiguration />
  <PublishedInformation>
    <SupportedVersions>
      <Version>1.0.0</Version>
      <Version>1.1.0</Version>
    </SupportedVersions>
    <EnableTransactions>true</EnableTransactions>
    <EnableStreaming>false</EnableStreaming>
    <QuerySRS>EPSG:4258</QuerySRS>
    <Format handler="GENERIC_GML">
      <MimeType>text/xml; subtype=gml/3.2.1</MimeType>
      <Param key="responseContainerLocalName">FeatureCollection</Param>
      <Param key="responseContainerPrefix">gml</Param>
      <Param key="responseContainerNs">http://www.opengis.net/gml/3.2</Param>
    </Format>
  </PublishedInformation>
</deegreeWFS>