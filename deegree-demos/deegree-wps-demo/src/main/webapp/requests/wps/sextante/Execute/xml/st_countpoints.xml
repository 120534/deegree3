<?xml version="1.0" encoding="UTF-8"?>
<wps:Execute xmlns:wps="http://www.opengis.net/wps/1.0.0"
  xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:xlink="http://www.w3.org/1999/xlink" service="WPS" version="1.0.0"
  xsi:schemaLocation="http://www.opengis.net/wps/1.0.0 http://schemas.opengis.net/wps/1.0.0/wpsExecute_request.xsd">
  <ows:Identifier>st_countpoints</ows:Identifier>
  <wps:DataInputs>
    <wps:Input>
      <ows:Identifier>POINTS</ows:Identifier>
      <wps:Data>
        <wps:ComplexData
          schema="http://schemas.opengis.net/gml/3.1.1/base/geometryComplexes.xsd"
          mimeType="text/xml">
          <gml:Point xmlns="http://www.opengis.net/gml"
            xmlns:gml="http://www.opengis.net/gml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            gml:id="Point1" srsName="EPSG:4326"
            xsi:schemaLocation="http://www.opengis.net/gml http://schemas.opengis.net/gml/3.1.1/base/geometryComplexes.xsd">
            <gml:coord>
              <gml:X>50</gml:X>
              <gml:Y>50</gml:Y>
            </gml:coord>
          </gml:Point>
        </wps:ComplexData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>POLYGONS</ows:Identifier>
      <wps:Data>
        <wps:ComplexData
          schema="http://schemas.opengis.net/gml/3.1.1/base/geometryComplexes.xsd"
          mimeType="text/xml">
          <gml:Polygon xmlns="http://www.opengis.net/gml"
            xmlns:gml="http://www.opengis.net/gml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            gml:id="Polygon14126" srsName="EPSG:4326"
            xsi:schemaLocation="http://www.opengis.net/gml http://schemas.opengis.net/gml/3.1.1/base/geometryComplexes.xsd">
            <gml:exterior>
              <gml:LinearRing>
                <gml:posList>0 0 100 0 100 100 0 100 0 0</gml:posList>
              </gml:LinearRing>
            </gml:exterior>
          </gml:Polygon>
        </wps:ComplexData>
      </wps:Data>
    </wps:Input>
  </wps:DataInputs>
  <wps:ResponseForm>
    <wps:ResponseDocument storeExecuteResponse="false"
      lineage="false" status="false">
      <wps:Output encoding="UTF-8"
        schema="http://schemas.opengis.net/gml/3.1.1/base/feature.xsd"
        mimeType="text/xml">
        <ows:Identifier>RESULT</ows:Identifier>
      </wps:Output>
    </wps:ResponseDocument>
  </wps:ResponseForm>
</wps:Execute>
