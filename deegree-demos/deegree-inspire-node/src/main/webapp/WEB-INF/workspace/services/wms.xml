<deegreeWMS xmlns="http://www.deegree.org/services/wms" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  configVersion="0.5.0" xsi:schemaLocation="http://www.deegree.org/services/wms http://schemas.deegree.org/wms/0.5.0/wms_configuration.xsd">
  <ServiceConfiguration>

    <UnrequestableLayer>
      <Title>Root Layer</Title>
      <CRS>EPSG:4258 EPSG:4326 EPSG:900913 EPSG:28992</CRS>

      <RequestableLayer>
        <Name>inspire-ad</Name>
        <Title>INSPIRE Addresses</Title>
        <FeatureStoreId>inspire-memory</FeatureStoreId>
        <DirectStyle>
          <File>../styles/Addresses.xml</File>
          <Name>addresses</Name>
        </DirectStyle>
      </RequestableLayer>

      <RequestableLayer>
        <Name>inspire-cp</Name>
        <Title>INSPIRE Cadastral Parcels</Title>
        <FeatureStoreId>inspire-memory</FeatureStoreId>
        <DirectStyle>
          <File>../styles/CadastralParcels.xml</File>
          <Name>cadastralparcels</Name>
        </DirectStyle>
      </RequestableLayer>

      <RequestableLayer>
        <Name>inspire-au</Name>
        <Title>INSPIRE AdministrativeUnits</Title>
        <LogicalLayer>
          <ScaleDenominators min="500000" max="100000000" />
          <FeatureStoreId>inspire-memory</FeatureStoreId>
          <DirectStyle>
            <File>../styles/AdministrativeUnits1.xml</File>
            <Name>inspire-au</Name>
          </DirectStyle>
        </LogicalLayer>
        <LogicalLayer>
          <ScaleDenominators min="100000" max="1000000" />
          <FeatureStoreId>inspire-memory</FeatureStoreId>
          <DirectStyle>
            <File>../styles/AdministrativeUnits2.xml</File>
            <Name>inspire-au</Name>
          </DirectStyle>
        </LogicalLayer>
        <LogicalLayer>
          <ScaleDenominators min="0" max="100000" />
          <FeatureStoreId>inspire-memory</FeatureStoreId>
          <DirectStyle>
            <File>../styles/AdministrativeUnits3.xml</File>
            <Name>inspire-au</Name>
          </DirectStyle>
        </LogicalLayer>
      </RequestableLayer>

    </UnrequestableLayer>
  </ServiceConfiguration>
</deegreeWMS>