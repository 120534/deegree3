<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="1.0">
  <info organisation="${module.organisation}" module="deegree-${module.name}" />
  <configurations>
    <conf name="${module.name}-build" description="Dependencies needed to build the core jar" />
    <conf name="${module.name}-javadoc" description="The source" visibility="private" />
    <conf name="${module.name}-src" description="The source" visibility="private" />
    <conf name="${module.name}-report" description="The report" visibility="private" />
  </configurations>
  <publications>
    <artifact name="${jar.name}" type="jar" conf="${module.name}-build" ext="jar" />
    <artifact name="${javadoc.name}" type="zip" conf="${module.name}-javadoc" />
    <artifact name="${src.name}" type="zip" conf="${module.name}-src" />
    <artifact name="${report.name}" type="html" conf="${module.name}-report" />
  </publications>
  <dependencies defaultconfmapping="${module.name}-build->default">
    <!-- Logging -->
    <dependency org="log4j" name="log4j" rev="1.2.15" transitive="false" />
    <dependency org="org.slf4j" name="slf4j-api" rev="1.5.11" />
    <dependency org="org.slf4j" name="slf4j-log4j12" rev="1.5.11" transitive="false" />
    <dependency org="org.slf4j" name="jcl-over-slf4j" rev="1.5.11" />
    <!-- JUnit -->
    <dependency org="junit" name="junit" rev="4.8.1" />
    <!-- XML -->
    <dependency org="jaxen" name="jaxen" rev="1.1.1" transitive="false" />
    <dependency org="xerces" name="xercesImpl" rev="2.9.1" />
    <dependency org="org.apache.ws.commons.axiom" name="axiom-impl" rev="1.2.9-deegreeversion" transitive="false" />
    <dependency org="org.apache.ws.commons.axiom" name="axiom-api" rev="1.2.9-deegreeversion" transitive="false" />
    <dependency org="com.sun.xml.bind" name="jaxb-impl" rev="2.1.10" />
    <dependency org="com.sun.xml.bind" name="jaxb-xjc" rev="2.1.10" />
    <dependency org="commons-lang" name="commons-lang" rev="2.0" />
    <dependency org="commons-dbcp" name="commons-dbcp" rev="1.2.2" />
    <dependency org="commons-httpclient" name="commons-httpclient" rev="3.1" />
    <dependency org="commons-pool" name="commons-pool" rev="1.4" />
    <dependency org="org.apache.commons" name="commons-math" rev="2.0" />
    <!--  JAI -->
    <dependency org="jai" name="jai-core" rev="1.1.3" />
    <dependency org="jai" name="jai-codec" rev="1.1.3" />
    <dependency org="jai" name="jai-imageio" rev="1.1.3" />
    <dependency org="jai" name="jai-mlibwrapper" rev="1.1.3" />
    <!-- AXIOM -->
    <dependency org="org.apache.ws.commons.axiom" name="axiom-impl" rev="1.2.8" transitive="false" />
    <dependency org="org.apache.ws.commons.axiom" name="axiom-api" rev="1.2.8" transitive="false" />
    <!-- Batik (compiling) -->
    <dependency org="batik" name="batik-bridge" rev="1.6-1" />
    <dependency org="batik" name="batik-dom" rev="1.6-1" />
    <dependency org="batik" name="batik-gvt" rev="1.6-1" />
    <dependency org="batik" name="batik-svg-dom" rev="1.6-1" />
    <!-- dependency org="batik" name="xml-apis-ext" rev="" transitive="false" conf="${module.name}-build" /-->
    <!-- Batik (additional for running) -->
    <!-- dependency org="batik" name="batik-anim" rev="" /-->
    <dependency org="batik" name="batik-awt-util" rev="1.6-1" />
    <dependency org="batik" name="batik-css" rev="1.6-1" />
    <dependency org="batik" name="batik-ext" rev="1.6-1" />
    <dependency org="batik" name="batik-parser" rev="1.6-1" />
    <dependency org="batik" name="batik-script" rev="1.6-1" />
    <dependency org="batik" name="batik-util" rev="1.6-1" />
    <dependency org="batik" name="batik-xml" rev="1.6-1" />
    <!-- Java 3D vecmath -->
    <dependency org="java3d" name="vecmath" rev="1.5.2" />
    <!-- The geometry package depends on JTS -->
    <dependency org="com.vividsolutions" name="jts" rev="1.10" />
    <dependency org="javax.mail" name="mail" rev="1.4.1" />
    <dependency org="net.sf.squirrel-sql.thirdparty-non-maven" name="java-cup" rev="0.11a" />
    <!--  PostgreSQL / PostGIS -->
    <dependency org="postgresql" name="postgresql" rev="8.4-701.jdbc4" />
    <dependency org="org.postgis" name="postgis-jdbc" rev="1.3.3" />
    <!-- JOGL -->
    <dependency org="jogl" name="jogl" rev="1.1.2" />
    <dependency org="jogl" name="gluegen-rt" rev="1.1.2" />
    <!-- Derby -->
    <dependency org="org.apache.derby" name="derby" rev="10.4.2.0" />
    <!-- ehcache -->
    <dependency org="net.sf.ehcache" name="ehcache" rev="1.8.0" />
    <dependency org="opengis-schemas" name="opengis-schemas" rev="2009-05-08" />
    <dependency org="jgridshift" name="jgridshift" rev="1.0-deegreeversion" />
  </dependencies>
</ivy-module>
