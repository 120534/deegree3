<MemoryFeatureStore configVersion="0.6.0" xmlns="http://www.deegree.org/datasource/feature/memory"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.deegree.org/datasource/feature/memory http://schemas.deegree.org/datasource/0.5.0/feature/memory.xsd">

  <!-- Prefixes to use for exporting qualified elements (features / properties) (0...n) -->
  <NamespaceHint namespaceURI="app" prefix="http://www.deegree.org/app" />

  <!-- GML schema files / directory to be used (1...n)-->
  <GMLSchemaFileURL gmlVersion="GML_32">...</GMLSchemaFileURL>

  <!-- GML datasets to load on startup (0...n)-->
  <GMLFeatureCollectionFileURL gmlVersion="GML_32">...</GMLFeatureCollectionFileURL>

</MemoryFeatureStore>