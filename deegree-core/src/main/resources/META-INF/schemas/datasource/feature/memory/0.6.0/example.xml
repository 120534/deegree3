<MemoryFeatureStore configVersion="0.6.0" xmlns="http://www.deegree.org/datasource/feature/memory"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.deegree.org/datasource/feature/memory http://schemas.deegree.org/datasource/feature/memory/0.6.0/memory.xsd">

  <!-- CRS for storing geometries [1] -->
  <StorageCRS>EPSG:4326</StorageCRS>

  <!-- Prefixes to use for exporting qualified elements (features / properties) [0...n] -->
  <NamespaceHint namespaceURI="app" prefix="http://www.deegree.org/app" />

  <!-- GML schema file / directory to be used [1...n] -->
  <GMLSchema version="GML_32">...</GMLSchema>

  <!-- GML datasets to load on startup [0...n] -->
  <GMLFeatureCollection version="GML_32">...</GMLFeatureCollection>

</MemoryFeatureStore>