<?xml version="1.0"?>
<FileSystemTileStore xmlns="http://www.deegree.org/datasource/tile/filesystem" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.deegree.org/datasource/tile/filesystem http://schemas.deegree.org/datasource/tile/filesystem/3.2.0/filesystem.xsd"
  configVersion="3.2.0" xmlns:g="http://www.deegree.org/metadata/spatial">

  <g:CRS>EPSG:26912</g:CRS>
  <g:Envelope crs="EPSG:26912">
    <g:LowerCorner>441174 4448455</g:LowerCorner>
    <g:UpperCorner>447810 4456039</g:UpperCorner>
  </g:Envelope>

  <!-- [1]: Geometry of the tile pyramid -->
  <TilePyramid>
    <!-- [1]: Width of a single tile in pixels -->
    <TileWidth>256</TileWidth>
    <!-- [1]: Height of a single tile in pixels -->
    <TileHeight>256</TileHeight>
    <!-- [1]: First scale level (base of the pyramid), scales of other levels are generated by subsequent division by two -->
    <MinScaleDenominator>7142.857142857143</MinScaleDenominator>
    <!-- [1]: Number of scale levels -->
    <NumLevels>5</NumLevels>
  </TilePyramid>

  <TileCacheDiskLayout>
    <LayerDirectory>../../data/tiles/filesystem/</LayerDirectory>
    <FileType>png</FileType>
  </TileCacheDiskLayout>

</FileSystemTileStore>
