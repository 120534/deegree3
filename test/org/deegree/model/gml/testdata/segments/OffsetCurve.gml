<?xml version="1.0" encoding="UTF-8"?>
<OffsetCurve xmlns="http://www.opengis.net/gml" xmlns:gml="http://www.opengis.net/gml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opengis.net/gml http://schemas.opengis.net/gml/3.1.1/base/geometryPrimitives.xsd">
	<offsetBase>
		<Curve>
			<segments>
				<LineStringSegment interpolation="linear">
					<posList srsName="EPSG:4326">2 0 0 2 -2 0</posList>
				</LineStringSegment>
			</segments>
		</Curve>
	</offsetBase>
	<distance uom="whatever#metres">1</distance>
	<refDirection>0.0 1.0</refDirection>
</OffsetCurve>
