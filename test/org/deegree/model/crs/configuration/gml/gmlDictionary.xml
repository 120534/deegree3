			<domainOfValidity xlink:href="urn:ogc:def:area:EPSG::1625" />
			<scope>Large and medium scale topographic mapping and
				engineering survey, cadastral survey.</scope>
			<conversion xlink:href="urn:ogc:def:coordinateOperation:EPSG::16263" />
			<baseGeodeticCRS xlink:href="urn:ogc:def:crs:EPSG::4314" />
