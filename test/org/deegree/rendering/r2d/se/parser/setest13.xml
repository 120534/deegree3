<?xml version="1.0"?>
<FeatureTypeStyle xmlns="http://www.opengis.net/se" xmlns:ogc="http://www.opengis.net/ogc" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1.0">
  <Rule>
    <PointSymbolizer />
    <TextSymbolizer>
      <Label>
        <Recode fallbackValue="#dddddd">
          <LookupValue>
            <ogc:PropertyName>TESTPROP</ogc:PropertyName>
          </LookupValue>
          <MapItem>
            <Data>0</Data>
            <Value>#000000</Value>
          </MapItem>
          <MapItem>
            <Data>255</Data>
            <Value>#ffffff</Value>
          </MapItem>
        </Recode>
      </Label>
      <Font>
        <SvgParameter name="font-size">20</SvgParameter>
      </Font>
    </TextSymbolizer>
  </Rule>
</FeatureTypeStyle>
