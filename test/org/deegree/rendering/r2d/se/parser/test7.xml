<?xml version="1.0"?>
<TextSymbolizer xmlns="http://www.opengis.net/sld"
                 xmlns:ogc="http://www.opengis.net/ogc"
                 xmlns:app="http://www.deegree.org/app">
  <Name>My Name</Name>
  <Geometry>app:someGeometryProp</Geometry>
  <Label>sometext</Label>
  <Font>
    <CssParameter name="font-family">times</CssParameter>
    <CssParameter name="font-weight">bold</CssParameter>
    <CssParameter name="font-size">8</CssParameter>
    <CssParameter name="font-style">italic</CssParameter>
  </Font>
  <LabelPlacement>
    <PointPlacement>
      <AnchorPoint>
        <AnchorPointX>87</AnchorPointX>
        <AnchorPointY>6</AnchorPointY>
      </AnchorPoint>
      <Displacement>
        <DisplacementX>3</DisplacementX>
        <DisplacementY>67</DisplacementY>
      </Displacement>
      <Rotation>78</Rotation>
    </PointPlacement>
    <LinePlacement>
      <PerpendicularOffset>786</PerpendicularOffset>
      <IsRepeated>true</IsRepeated>
      <InitialGap>8</InitialGap>
      <Gap>7.3</Gap>
      <IsAligned>false</IsAligned>
      <GeneralizeLine>true</GeneralizeLine>
    </LinePlacement>
  </LabelPlacement>
</TextSymbolizer>
