            </gmd:dateType>
          </gmd:CI_Date>
        </gmd:date>
      </gmd:CI_Citation>
    </gmd:specification>
    <gmd:explanation>
      <gco:CharacterString>See the referenced specification</gco:CharacterString>
    </gmd:explanation>
    <gmd:pass
      gco:nilReason="template"/>
  </inspire_ds:Conformity>
  <inspire_ds:Conformity>
    <gmd:specification>
      <gmd:CI_Citation>
        <gmd:title>
          <gco:CharacterString>http://eur-lex.europa.eu/LexUriServ/LexUriServ.do?uri=CELEX:32008R1205:EN:NOT
          </gco:CharacterString>
        </gmd:title>
        <gmd:date>
          <gmd:CI_Date>
            <gmd:date>
              <gco:Date>2008-12-03</gco:Date>
            </gmd:date>
            <gmd:dateType>
              <gmd:CI_DateTypeCode
                codeListValue="publication"
                codeList="http://standards.iso.org/ittf/PubliclyAvailableStandards/ISO_19139_Schemas/resources/Codelist/ML_gmxCodelists.xml#CI_DateTypeCode">publication</gmd:CI_DateTypeCode>
            </gmd:dateType>
          </gmd:CI_Date>
        </gmd:date>
      </gmd:CI_Citation>
    </gmd:specification>
    <gmd:explanation>
      <gco:CharacterString>See referenced specification</gco:CharacterString>
    </gmd:explanation>
    <gmd:pass gco:nilReason="template"/>
  </inspire_ds:Conformity>
  <inspire_ds:MetadataPointOfContact>
    <gmd:organisationName>
      <gco:CharacterString>${ORGANIZATION_NAME}</gco:CharacterString>
    </gmd:organisationName>
    <gmd:contactInfo>
      <gmd:CI_Contact>
        <gmd:address>
          <gmd:CI_Address>
            <gmd:electronicMailAddress>
              <gco:CharacterString>${CI_EMAIL_ADDRESS}</gco:CharacterString>
            </gmd:electronicMailAddress>
          </gmd:CI_Address>
        </gmd:address>
      </gmd:CI_Contact>
    </gmd:contactInfo>
    <gmd:role>
      <gmd:CI_RoleCode
        codeListValue="pointOfContact"
        codeList="http://standards.iso.org/ittf/PubliclyAvailableStandards/ISO_19139_Schemas/resources/Codelist/ML_gmxCodelists.xml#CI_RoleCode">pointOfContact</gmd:CI_RoleCode>
    </gmd:role>
  </inspire_ds:MetadataPointOfContact>
  <inspire_ds:MetadataDate>${SYSTEM_START_DATE}</inspire_ds:MetadataDate>
  <inspire_ds:SpatialDataServiceType>
    <gco:LocalName codeSpace="INSPIRE">discovery</gco:LocalName>
  </inspire_ds:SpatialDataServiceType>
  <inspire_ds:InspireKeywords>
    <gmd:keyword>
      <gco:CharacterString>humanCatalogueViewer</gco:CharacterString>
    </gmd:keyword>
  </inspire_ds:InspireKeywords>
  <inspire_ds:Languages>
    <inspire_ds:Language default="true">eng</inspire_ds:Language>
  </inspire_ds:Languages>
  <inspire_ds:CurrentLanguage>eng</inspire_ds:CurrentLanguage>
</inspire_ds:ExtendedCapabilities>