<?xml version="1.0" encoding="UTF-8"?>
<inspire_ds:ExtendedCapabilities
  xmlns:inspire_ds="http://inspire.europa.eu/networkservice/discovery"
  xmlns:gmd="http://www.isotc211.org/2005/gmd"
  xmlns:gml="http://www.opengis.net/gml/3.2"
  xmlns:gco="http://www.isotc211.org/2005/gco"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <inspire_ds:ResourceLocator>
    <gmd:onLine>
      <gmd:CI_OnlineResource>
        <gmd:linkage>
          <gmd:URL>${SERVER_ADDRESS}REQUEST=GetCapabilities&amp;SERVICE=CSW&amp;VERSION=2.0.2</gmd:URL>
        </gmd:linkage>
      </gmd:CI_OnlineResource>
    </gmd:onLine>
  </inspire_ds:ResourceLocator>
  <!--
    <inspire_ds:MetadataUrl> <gmd:linkage>
    <gmd:URL>$SERVER-ADRESS?Service=CSW&amp;Request=GetRecordById&amp;Version=2.0.2&amp;id=$OWN_DATASET_ID&amp;outputSchema=http://www.isotc211.org/2005/gmd&amp;elementSetName=full</gmd:URL>
    </gmd:linkage> </inspire_ds:MetadataUrl>
  -->
  <inspire_ds:ResourceType>
    <gmd:MD_ScopeCode
      codeListValue="service"
      codeList="http://standards.iso.org/ittf/ PubliclyAvailableStandards/ISO_19139_Schemas/ resources/Codelist/gmxCodelists.xml#MD_ScopeCode">service</gmd:MD_ScopeCode>
  </inspire_ds:ResourceType>
  <inspire_ds:TemporalReference>
    <gmd:temporalElement>
      <gmd:EX_TemporalExtent>
        <gmd:extent>
          <gml:TimePeriod
            gml:id="TIMEPERIOD_ID"
            xsi:type="gml:TimePeriodType">
            <gml:beginPosition>${SYSTEM_START_DATE}</gml:beginPosition>
            <gml:endPosition>2100-01-01</gml:endPosition>
          </gml:TimePeriod>
        </gmd:extent>
      </gmd:EX_TemporalExtent>
    </gmd:temporalElement>
  </inspire_ds:TemporalReference>
  <inspire_ds:Conformity>
    <gmd:specification>
      <gmd:CI_Citation>
        <gmd:title>
          <gco:CharacterString>http://eur-lex.europa.eu/LexUriServ/LexUriServ.do?uri=CELEX:32009R0976:EN:NOT
          </gco:CharacterString>
        </gmd:title>
        <gmd:date>
          <gmd:CI_Date>
            <gmd:date>
              <gco:Date>2009-10-12</gco:Date>
            </gmd:date>
            <gmd:dateType>
              <gmd:CI_DateTypeCode
